{
    "name": "ots/backend",
    "description": "Backend for Online Travel Support",
    "keywords": ["ots", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-soap": "*",
        "ext-curl": "*",
        "laravel/framework": "^11.0",
        "dyrynda/laravel-cascade-soft-deletes": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0",
        "laravel/sail": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Extensions\\": "extensions/",
            "Modules\\": "modules/"
        },
        "files": ["app/Helper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Procedure\\": "tests/procedure",
            "Tests\\Procedure\\Ots\\PriceModifier\\": "tests/procedure/ots/price_modifier",
            "Tests\\Procedure\\Ots\\Offer\\": "tests/procedure/ots/offer"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
