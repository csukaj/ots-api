{
    "name": "ots/backend",
    "description": "Backend for Online Travel Support",
    "keywords": ["ots", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.0",
        "ext-json": "*",
        "ext-soap": "*",
        "ext-curl": "*",
        "laravel/framework": "5.4.27",
        "barryvdh/laravel-cors": "0.11.0",
        "zizaco/entrust": "1.9.1",
        "tymon/jwt-auth": "1.0.0-rc.1",
        "nwidart/laravel-modules": "1.27.2",
        "predis/predis": "1.1.1",
        "guzzlehttp/guzzle": "6.3.3",
        "baum/baum": "1.1.1",
        "umpirsky/country-list": "2.0.4",
        "google/recaptcha": "1.1.3",
        "mledoze/countries": "1.8.0",
        "doctrine/dbal": "2.6.3",
        "mpociot/laravel-apidoc-generator": "2.0.0",
        "iatstuti/laravel-cascade-soft-deletes": "1.4.0",
        "artisaninweb/laravel-soap": "0.3.0.9"
    },
    "require-dev": {
        "fzaninotto/faker": "1.7.1",
        "mockery/mockery": "1.1.0",
        "phpunit/phpunit": "7.3.5",
        "symfony/css-selector": "3.4.9",
        "symfony/dom-crawler": "3.4.9",
        "laravel/homestead": "3.1.0",
        "vicebas/php-docblock-generator": "1.0.1",
        "barryvdh/laravel-ide-helper": "2.4.3",
        "doctrine/dbal": "2.6.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Extensions\\": "extensions/",
            "Modules\\": "modules/"
        },
        "files": ["app/Helper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Procedure\\": "tests/procedure",
            "Tests\\Procedure\\Ots\\PriceModifier\\": "tests/procedure/ots/price_modifier",
            "Tests\\Procedure\\Ots\\Offer\\": "tests/procedure/ots/offer"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
