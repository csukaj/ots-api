{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.2"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico",
            "headers": {
                "Cache-Control": "max-age=31536000"
            }
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/docs/(.*)",
            "dest": "/public/docs/$1",
            "headers": {
                "Cache-Control": "max-age=86400"
            }
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://ots-api.vercel.app",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "VIEW_COMPILED_PATH": "/tmp/storage/framework/views",
        "CACHE_PATH": "/tmp/storage/framework/cache",
        "STORAGE_PATH": "/tmp/storage"
    },
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "maxDuration": 30
        }
    }
}